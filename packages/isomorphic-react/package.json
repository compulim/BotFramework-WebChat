{
  "name": "isomorphic-react",
  "version": "0.0.0-0",
  "main": "dist/react.js",
  "private": true,
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel src --out-dir lib --verbose",
    "build:webpack": "webpack-cli",
    "eslint": "npm run precommit",
    "precommit": "concurrently \"npm run precommit:*\"",
    "precommit:eslint": "eslint src/**/*.{js,ts}",
    "prettier": "prettier --check src/**/*.{js,ts}",
    "start": "npm run start:note && npm run start:babel && npm run start:webpack",
    "start:babel": "npm run build:babel",
    "start:note": "echo \"isomorphic-react is not under active development; running in non-watch mode.\"",
    "start:webpack": "npm run build:webpack"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.11.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "prettier": "^2.1.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
